<?xml version="1.0"?>
<Configuration name="Log4j2">
    <Appenders>
        <Console name="stdout" target="SYSTEM_OUT">
            <PatternLayout pattern="[%d] %p %m (%c:%L)%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Root level="INFO">
            <AppenderRef ref="stdout" />
        </Root>
    </Loggers>
</Configuration>